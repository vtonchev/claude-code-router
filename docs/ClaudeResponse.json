{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Claude Stream Event",
  "description": "Schema for Anthropic Claude streaming events including thinking blocks and signatures",
  "type": "object",
  "required": [
    "type"
  ],
  "properties": {
    "type": {
      "type": "string",
      "enum": [
        "message_start",
        "content_block_start",
        "content_block_delta",
        "content_block_stop",
        "message_delta",
        "message_stop",
        "ping",
        "error"
      ]
    }
  },
  "oneOf": [
    {
      "title": "Message Start",
      "properties": {
        "type": {
          "const": "message_start"
        },
        "message": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string"
            },
            "type": {
              "const": "message"
            },
            "role": {
              "type": "string",
              "enum": [
                "assistant"
              ]
            },
            "content": {
              "type": "array",
              "default": []
            },
            "model": {
              "type": "string"
            },
            "stop_reason": {
              "type": [
                "string",
                "null"
              ]
            },
            "stop_sequence": {
              "type": [
                "string",
                "null"
              ]
            },
            "usage": {
              "$ref": "#/definitions/usage"
            },
            "service_tier": {
              "type": "string"
            }
          },
          "required": [
            "id",
            "type",
            "role",
            "model",
            "usage"
          ]
        }
      },
      "required": [
        "type",
        "message"
      ]
    },
    {
      "title": "Content Block Start",
      "properties": {
        "type": {
          "const": "content_block_start"
        },
        "index": {
          "type": "integer"
        },
        "content_block": {
          "type": "object",
          "properties": {
            "type": {
              "type": "string",
              "enum": [
                "text",
                "thinking"
              ]
            },
            "text": {
              "type": "string"
            },
            "thinking": {
              "type": "string"
            },
            "signature": {
              "type": "string"
            }
          },
          "required": [
            "type"
          ]
        }
      },
      "required": [
        "type",
        "index",
        "content_block"
      ]
    },
    {
      "title": "Content Block Delta",
      "properties": {
        "type": {
          "const": "content_block_delta"
        },
        "index": {
          "type": "integer"
        },
        "delta": {
          "type": "object",
          "properties": {
            "type": {
              "type": "string",
              "enum": [
                "text_delta",
                "thinking_delta",
                "signature_delta"
              ]
            },
            "text": {
              "type": "string"
            },
            "thinking": {
              "type": "string"
            },
            "signature": {
              "type": "string"
            }
          },
          "required": [
            "type"
          ]
        }
      },
      "required": [
        "type",
        "index",
        "delta"
      ]
    },
    {
      "title": "Content Block Stop",
      "properties": {
        "type": {
          "const": "content_block_stop"
        },
        "index": {
          "type": "integer"
        }
      },
      "required": [
        "type",
        "index"
      ]
    },
    {
      "title": "Message Delta",
      "properties": {
        "type": {
          "const": "message_delta"
        },
        "delta": {
          "type": "object",
          "properties": {
            "stop_reason": {
              "type": [
                "string",
                "null"
              ]
            },
            "stop_sequence": {
              "type": [
                "string",
                "null"
              ]
            }
          },
          "required": [
            "stop_reason",
            "stop_sequence"
          ]
        },
        "usage": {
          "type": "object",
          "properties": {
            "output_tokens": {
              "type": "integer"
            }
          },
          "required": [
            "output_tokens"
          ]
        },
        "context_management": {
          "type": "object",
          "properties": {
            "applied_edits": {
              "type": "array"
            }
          }
        }
      },
      "required": [
        "type",
        "delta",
        "usage"
      ]
    },
    {
      "title": "Message Stop",
      "properties": {
        "type": {
          "const": "message_stop"
        }
      },
      "required": [
        "type"
      ]
    },
    {
      "title": "Ping",
      "properties": {
        "type": {
          "const": "ping"
        }
      },
      "required": [
        "type"
      ]
    }
  ],
  "definitions": {
    "usage": {
      "type": "object",
      "properties": {
        "input_tokens": {
          "type": "integer"
        },
        "output_tokens": {
          "type": "integer"
        },
        "cache_creation_input_tokens": {
          "type": "integer"
        },
        "cache_read_input_tokens": {
          "type": "integer"
        },
        "cache_creation": {
          "type": "object",
          "properties": {
            "ephemeral_5m_input_tokens": {
              "type": "integer"
            },
            "ephemeral_1h_input_tokens": {
              "type": "integer"
            }
          }
        }
      },
      "required": [
        "input_tokens",
        "output_tokens"
      ]
    }
  }
}
